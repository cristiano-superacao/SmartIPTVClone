{
  "name": "smartiptv-clone",
  "version": "2.0.0",
  "description": "Clone profissional do SmartIPTV com suporte multiplataforma",
  "main": "App.js",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "react-native-web start",
    "build:web": "react-native-web build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "rm -rf node_modules && npm install",
    "clean:cache": "npm start -- --reset-cache",
    "backend": "cd backend && npm start",
    "backend:dev": "cd backend && npm run dev",
    "dev": "concurrently \"npm run backend:dev\" \"npm start\""
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-native": "^0.72.0",
    "react-native-video": "^5.2.1",
    "react-native-web": "^0.19.0",
    "@react-navigation/native": "^6.1.0",
    "@react-navigation/stack": "^6.3.0",
    "axios": "^1.6.0",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-reanimated": "^3.6.0",
    "react-native-safe-area-context": "^4.8.0",
    "react-native-screens": "^3.29.0",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-community/netinfo": "^11.2.0",
    "react-native-fast-image": "^8.6.3",
    "i18next": "^23.7.0",
    "react-i18next": "^13.5.0",
    "lodash": "^4.17.21",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-vector-icons": "^10.0.3",
    "react-native-modal": "^13.0.1",
    "react-native-toast-message": "^2.2.0",
    "react-native-orientation-locker": "^1.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/runtime": "^7.23.0",
    "@react-native/eslint-config": "^0.72.0",
    "@react-native/metro-config": "^0.72.0",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.2.0",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.0",
    "metro-react-native-babel-preset": "^0.76.0",
    "prettier": "^2.8.0",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16"
  }
}
